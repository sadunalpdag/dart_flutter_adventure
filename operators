main() {

var toplama = 4+3;

print(toplama);

var bolme =10/2; //bu kusuratl覺 sonuc verir~/ yap覺l覺rsa yuvarlar

print(bolme);

var kalan =4%3;



print(kalan);

print('kalan:${4%3}');

print('kalan:${kalan}');

print(kalan+1);

kalan+=2;
print(kalan);

kalan-=2;

print(kalan);

kalan-=1;

print(kalan);

var n=0;

n++;

print(n);

var a=3;
print(a);

int x=a++; // burada cycle dan sonra + 1 art覺s olur
print(a);

print(x);

  
}

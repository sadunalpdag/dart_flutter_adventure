class hesaplama {
  int age;
  String name;
  hesaplama(
    {age, name
      
      //braket burada kullanıyorsun fonksiyon cağırıken kullanmıyorsun 
  }){
    
    this.age = age;
    this.name = name;
    
    //sonra burayıda yukarıya tasıyoruz.... gerek yok ama yazılabilir
  }
    
    letsgo(){print('goaway');}
}

addnumber(num1,num2){return num1+num2;}

//return sonuc olusturur fonksiyonda 
void main() {
 print (addnumber(1, 2));
//print bildiğin print
  
  hesaplama kare =hesaplama(age:30, name:"sadun");
  
  kare.letsgo();
  
  print(kare.age);
  print (kare.age+1);
  
  print (kare.age+kare.age); 
  

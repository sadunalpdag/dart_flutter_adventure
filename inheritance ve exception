class hesaplama {
  int age;
  String name;
  hesaplama(
    {age, name
      
      //braket burada kullanıyorsun fonksiyon cağırıken kullanmıyorsun 
  }){
    
    this.age = age;
    this.name = name;
    
    //sonra burayıda yukarıya tasıyoruz.... gerek yok ama yazılabilir
  }
    
    letsgo(){print('goaway');
     dontgo(){throw new FormatException();}       }
}

class supercalc extends hesaplama {supercalc(age,name): super (age:age,name:name);
dontgo(){print('dontgo');}
                                  
                                  }

addnumber(num1,num2){return num1+num2;}

